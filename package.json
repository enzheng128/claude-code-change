{
  "name": "claude-code-change",
  "displayName": "Claude Code Change",
  "description": "A VSCode extension that provides file comparison and change tracking functionality for Claude Code conversations",
  "version": "1.0.1",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/src/extension.js",
  "contributes": {
    "views": {
      "claude-code-change-view": [
        {
          "id": "fileTypeExplorer",
          "name": "Claude Code Change",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "claude-code-change-view",
          "title": "claude code change",
          "icon": "$(arrow-both)"
        }
      ]
    },
    "commands": [
      {
        "command": "fileTypeExplorer.openFile",
        "title": "Open File"
      },
      {
        "command": "fileTypeExplorer.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "claudeCodeChange.install",
        "title": "Claude Code Change: Install Tools"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "fileTypeExplorer.refresh",
          "when": "view == fileTypeExplorer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "mkdir -p dist && vsce package --out dist/"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "author": "",
  "license": "ISC",
  "publisher": "claude-code-change",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/claude-code-change.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/claude-code-change/issues"
  },
  "homepage": "https://github.com/your-username/claude-code-change#readme"
}